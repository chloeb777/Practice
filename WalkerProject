    if ((dist(walkerArray[i].myX, walkerArray[i].myY, walkerArray[(1)].myX, walkerArray[(1)].myY) <= 50)||(dist(walkerArray[i].myX, walkerArray[i].myY, walkerArray[(2)].myX, walkerArray[(2)].myY) <= 50)||(dist(walkerArray[i].myX, walkerArray[i].myY, walkerArray[(3)].myX, walkerArray[(3)].myY) <= 50)||(dist(walkerArray[i].myX, walkerArray[i].myY, walkerArray[(4)].myX, walkerArray[(4)].myY) <= 50)||(dist(walkerArray[i].myX, walkerArray[i].myY, walkerArray[(5)].myX, walkerArray[(5)].myY) <= 50)||(dist(walkerArray[i].myX, walkerArray[i].myY, walkerArray[(6)].myX, walkerArray[(6)].myY) <= 50)||(dist(walkerArray[i].myX, walkerArray[i].myY, walkerArray[(7)].myX, walkerArray[(7)].myY) <= 50)||(dist(walkerArray[i].myX, walkerArray[i].myY, walkerArray[(8)].myX, walkerArray[(8)].myY) <= 50)||(dist(walkerArray[i].myX, walkerArray[i].myY, walkerArray[(9)].myX, walkerArray[(9)].myY) <= 50)){
